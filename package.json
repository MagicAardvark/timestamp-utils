{
  "name": "timestamp-utils",
  "version": "1.0.0",
  "description": "⏱️ An anwsome and tiny util package for timestamp without dependencies",
  "main": "dist/index.js",
  "ava": {
    "require": ["babel-register"]
  },
  "scripts": {
    "build": "$(npm bin)/babel src --out-dir dist",
    "watch": "$(npm bin)/babel src --watch --out-dir dist",
    "size": "$(npm bin)/size-limit",
    "test": "ava",
    "prepublishOnly": "npm run test && npm run build && npm run size"
  },
  "keywords": [
    "timestamp",
    "utils"
  ],
  "author": "Arnaud CETOUTE <arnaud.cetoute@hotmail.fr>",
  "license": "MIT",
  "devDependencies": {
    "ava": "0.25.0",
    "babel-cli": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-minify": "0.4.3",
    "babel-register": "6.26.0",
    "size-limit": "0.18.5"
  }
}
